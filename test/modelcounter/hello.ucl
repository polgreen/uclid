module main {

  define f(n : integer) : boolean = n >= 5 && n <= 10;
  define g(n : integer) : boolean = n >= 11 && n <= 20;
  define h(n : integer) : boolean = n >= 5 && n <= 20;

  proof {
    assert or: #[(n:integer) :: f(n)] == 
               #[(n:integer) :: g(n)] + #[(n:integer) :: h(n)];
  }
}

