module main {

    var a, b, c, d : integer;

    procedure works()
        modifies a,b;
        requires old(a) > old(b);
        ensures b > a;
    {
        var temp : integer;

        temp = a;
        a = b;
        b = temp;
    }

    procedure doesnt_work()
        modifies c,d;
        requires c > d;
        ensures d > c;
    {
        var temp : integer;

        temp = c;
        c = d;
        d = temp;
    }

    next {
        assume (a > b);
        assume (c > d);

        call works();
        call doesnt_work();
    }


    control {
        v = induction(1);
        check;
        print_results;
        v.print_cex();
    }
}