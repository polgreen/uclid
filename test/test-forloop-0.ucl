module main {
  var cnt, cntp, cntq : integer;
  const three = 3;
  const one = 1;

  procedure add10(x : integer) returns (y : integer) {
    y = x;
    for i in range(1, 3) {
      for j in range(1, 3) {
        y = y + one;
      }
    }
    y = y + one;
  }

  procedure add10b(x : integer) returns (y : integer) {
    y = x;
    for i in range(1, 4) {
      y = y + i;
    }
  }
  
  init {
    cnt = 0;
    cntp = 0;
    cntq = 0;
  }

  next {
    call (cnt') = add10(cnt);
    call (cntq') = add10b(cntq);
    cntp' = cntp + 10;
    
    // noteToSelf(ReorderingTest)
    assert (cnt' == cntp');
    assert (cntq' == cntp');
  }

  control {
    // print_module;
    unroll (5);
    check;
    // print_results;
  }
}
