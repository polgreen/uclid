module main {
  var cnt, cntp, cntq : int;
  
  
  procedure add10(x : int) returns (y : int) {
    y = x;
    for i in range(1, 3) {
      for j in range(1, 3) {
        y = y + 1;
      }
    }
    y = y + 1;
  }
  
  procedure add10b(x : int) returns (y : int) {
    y = x;
    for i in range(1, 4) {
      y = y + i;
    }
  }
  
  init {
    cnt = 0;
    cntp = 0;
    cntq = 0;
  }
  next {
    call (cnt) = add10(cnt);
    call (cntq) = add10b(cntq);
    cntp = cntp + 10;
    
    
    assert (cnt == cntp);
    assert (cntq == cntp);
  }
  control {
    unroll (5);
    decide;
  }
}
