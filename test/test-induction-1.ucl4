module main {
  var x, y : int;
  
  init {
    x := 1;
    y := 1;
  }
  
  next {
    y := y + 1;
    x := x + y;
  }
  
  property ind_strengthen : x >= 0 && y >= 0;
  property y_le_x : y <= x;
  
  control {
  	// base case.
    k_induction_base (1);
    decide;
    print_results;
    clear_context;
    
    // inductive step.
    k_induction_step (1);
    decide;
  }
}