module child
{
  procedure add(a : integer, b : integer) returns (c : integer)
    requires (a >= 0 && a < 10);
    requires (b >= 0 && b < 10);
    ensures c == a + b;
    ensures (c >= 0 && c < 18);
  {
    c = a + b;
  }
}


module main
{

  instance chld : child();

  procedure child_add(d : integer, e : integer) returns (f : integer)
    requires (d >= 0 && d < 10);
    requires (e >= 0 && e < 10);
    ensures f == d + e;
    ensures (f >= 0 && f < 18);
    modifies chld;
  {
    call (f) = chld.add(d, e);
    assert(false);
  }

  control {
      add_verif = verify(child_add);
      //sum_verif = verify(sum);
      check;
      print_results;
      //add_verif.print_cex(a, b, c);
      // sum_verif->print_cex(n, x, numbers[1], numbers[2], numbers[3], numbers[4], numbers[5]);
  }


}


