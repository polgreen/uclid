module main
{
  input n  : int;
  var nums : [int]int;
  var x : int; 

  procedure sum(n : int) returns (numbers : [int]int)
    requires (n > 0);
    requires (n < 5);
    ensures (forall (i : int) :: (i > 0 && i < 5) ==> numbers[i] == i);
    modifies z;
  {
    for i in range(1, 5) {
      if (i < n) {
        numbers[i] = i;
      }
    }
    x = x + 1;
  }

  next {
    call (nums) = sum(n);
  }

  control {
    print_module;
  }
}