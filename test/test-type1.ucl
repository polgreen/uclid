module test {
  var a, b : int;

  procedure fib(x: int, y: int) returns (z:int, w:int) { 
    z, w = y, x + y;
  }
  init {
    a = 0;
    b = 1;
  }
  next {
    call (a, b) = fib(a, b);
  }
  
  control {
  	simulate (5);
  }
}

