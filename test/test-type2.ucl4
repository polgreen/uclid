module main {
  type hash_t;
  
  function hash_int(x : int) : hash_t;
  axiom (forall (x1 : int, x2 : int) :: hash_int(x1) != hash_int(x2));
  
  
  input i : int;
  var v1 : hash_t;
  var v2 : hash_t;
  
  init {
    v1 := hash_int(0);
    v2 := hash_int(1);
  }
  
  next {
    v1 := hash_int(i);
    v2 := hash_int(i+1);
  }
  
  property f_neq : v1 != v2;
  
  control {
    initialize;
    simulate(3);
    decide;
    print_results;
  }
}

    