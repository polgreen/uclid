// Test that a havoc statement that refers to an non-existent variable results in a compiler error.

module main {
  var a : int;
  var b : int;
  procedure fib(x: int, y: int) returns (z:int, w:int) { 
    z, w = y, x + y;
  }
  init {
    a = 0;
    b = 1;
  }
  next {
    call (a, b) = fib(a, b);
    havoc z;
  }
  
  control {
    initialize;
  	simulate (5);
  }
}
