// Test that a for loop with invalid ranges results in a compiler error.

module main {
  var a : int;
  var b : int;

  var c : bv16;
  var d : bv8;
  var e : bv8;

  init {
    a := 0;
    b := 1;
    for i in range(10, 1)  {
      a := a + 1;
    }
    for i in range(c, d)  {
      a := a + 1;
    }
    for i in range(10bv8, 0bv8)  {
      a := a + 1;
    }
  }
  next {
    a := a + 1;
  }

  control {
    initialize;
  	simulate (5);
  }
}