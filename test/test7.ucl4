module testModule {
       
  var v1: int;
  var arr: [int] int;
  type t1 = int;
  type t2 = record{f1: int, f2: bool};

  procedure p1(a1: int, a2: int) {
    v1 := v1 + a1 + arr[a2];
  }
  procedure p2(a3: int) {
    arr[a3] := arr[a3] + 4;
  }

  init {
    havoc v1;
  }
  next {
    call () := p1(1,2);
    call () := p2(7);
  }
}
