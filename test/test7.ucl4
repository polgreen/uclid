module testModule {
       
  var v1: int;
  var arr: [int,int] int;
  var f: int -> int;
  type t1 = int;

  procedure p1(a1: int, a2: int) {
    v1 := v1 + a1 + arr[a2,a2];
  }
  procedure p2(a3: int) {
    arr[a3,a3] := arr[a3,a3] + 4;
  }

  procedure p3(a: int) {
    f := Lambda(i:int). ITE(i > 0, f(i), i);
  }

  init {
    havoc v1;
    havoc arr;
    f := Lambda(i:int). 0;
  }
  next {
    call () := p1(1,2);
    call () := p2(7);
    assert f(v1) < 5;
  }
}
